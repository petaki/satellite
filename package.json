{
  "name": "carrier",
  "version": "1.0.0",
  "description": "Dashboard for probes.",
  "productName": "Carrier",
  "main": "build/index.js",
  "repository": "git@github.com:petaki/carrier.git",
  "author": "Peter Takacs <peter.takacs@koodilab.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc",
    "build-res": "cross-env NODE_ENV=production parcel build -t electron -d build --public-url ./ --no-source-maps resources/index.html",
    "dist": "electron-packager . --asar --out=./dist",
    "start": "electron .",
    "watch": "tsc -w",
    "watch-res": "parcel -t electron -d build --public-url ./ resources/index.html"
  },
  "dependencies": {
    "chart.js": "^2.7.3",
    "electron-devtools-installer": "^2.2.4",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "redis": "^2.8.0",
    "ssh2": "^0.6.1",
    "vue": "^2.5.21",
    "vue-property-decorator": "^7.2.0",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@types/chart.js": "^2.7.41",
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/lodash": "^4.14.118",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.12.11",
    "@types/redis": "^2.8.8",
    "@types/ssh2": "^0.5.37",
    "@vue/component-compiler-utils": "^2.3.0",
    "cross-env": "^5.2.0",
    "electron": "^3.0.10",
    "electron-packager": "^13.0.1",
    "node-sass": "^4.10.0",
    "parcel-bundler": "^1.10.3",
    "postcss": "^7.0.6",
    "tailwindcss": "^0.7.2",
    "tslint": "^5.11.0",
    "typescript": "^3.2.1",
    "vue-class-component": "^6.3.2",
    "vue-hot-reload-api": "^2.3.1",
    "vue-template-compiler": "^2.5.21"
  }
}
